(echo "digraph {" && (perl -ne 'print if s!.*?"(GET|POST) (/.*?)[ #&?;].*?" 2.*?"\w+?://(?:intranet-if.*?(?=/))?(.*?)["#&?;].*"!"\3" -> "\2"!' ../test.log | sort | uniq -c | perl -pe 's! *([0-9]+) (.*)!    \2 [label = "\1"];!') && echo "}")| sort > fichier.dot.outfile && ../../bin/analog -g fichier.dot ../test.log && sort fichier.dot -o fichier.dot
